{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_GEMINI_API_KEY": "@gemini-api-key"
  },
  "functions": {
    "app/api/**": {
      "maxDuration": 30
    }
  }
}